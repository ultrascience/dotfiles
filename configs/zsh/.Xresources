URxvt.keysym.Control-Shift-C: eval:selection_to_clipboard
URxvt.keysym.Control-Shift-V: eval:paste_clipboard
! -----------------------------------------------------------------------------------------------------------------

URxvt*skipBuiltinGlyphs: false

URxvt.is14755: false
URxvt.iso14755_52: false
URxvt*termName: rxvt-unicode-256color

! terminal padding
URxvt.internalBorder: 1
 
URxvt.transparent:    true
URxvt.shading:     10
 
URxvt.matcher.rend.0: Uline Bold fg5
URxvt.letterSpace: 0
URxvt.lineSpace: 0
URxvt.depth:                      30
URxvt*scrollBar:                  false
URxvt*mouseWheelScrollPage:       false
URxt*cursorBlink:                true
URxvt*saveLines:                  1000000
URxvt.urgentOnBell: true

! Font
! -----------------------------------------------------------------------------------------------------------------
URxvt*font: xft:JetBrainsMono Nerd Font Mono:pixelsize=13:antialias=true
! -----------------------------------------------------------------------------------------------------------------

! Cursor size
Xcursor.size: 5
Xft.autohint: false
Xft.lcdfilter:  lcddefault
Xft.hintstyle:  hintslight
Xft.hinting: 1
Xft.antialias: true
Xft.rgba: rgb

xterm*.foreground:    #A9B1D6
xterm*.background:    #1A1B26
xterm*.cursorColor:   #A9B1D6
xterm*.highlightColor:#3A3F4B

